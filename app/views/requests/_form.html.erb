<%= simple_form_for(request) do |f| %>
  <%= f.label :first_name %>
  <%= f.input :first_name %>
  <%= f.label :last_name %>
  <%= f.input :last_name %>
  <%= f.label :phone_number %>
  <%= f.input :phone_number %>
  <%= f.label :email %>
  <%= f.input :email %>
  <%= f.label :biography %>
  <%= f.input :biography %>
  <%= f.submit %>
<% end %>
